#!usr/bin/python3
class Solution:
    # @param s, a string
    # @return a boolean
    def isPalindrome(self, s):
        s = str(input())
        alpha_s = "".join([letter for letter in s if letter.isalpha()])
        if alpha_s == alpha_s[::-1]:
        	return True
        else:
        	return False
